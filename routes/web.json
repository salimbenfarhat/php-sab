{
  "routes": [
    {
      "path": "/",
      "controller": "FrontController",
      "method": "home"
    },
    {
      "path": "/a-propos",
      "controller": "FrontController",
      "method": "about"
    },
    {
      "path": "/auth",
      "controller": "AuthController",
      "method": "redirectToLogin"
    },
    {
      "path": "/auth/connexion",
      "controller": "AuthController",
      "method": "login"
    },
    {
      "path": "/auth/inscription",
      "controller": "AuthController",
      "method": "signup"
    },
    {
      "path": "/auth/inscription/confirmation",
      "controller": "AuthController",
      "method": "signupConfirmation"
    },
    {
      "path": "/auth/inscription/verification",
      "controller": "AuthController",
      "method": "signupAccountValidation"
    },
    {
      "path": "/auth/inscription/verification/:token",
      "controller": "AuthController",
      "method": "signupAccountValidation"
    },
    {
      "path": "/auth/inscription/verification-reussie",
      "controller": "AuthController",
      "method": "signupVerifySuccess"
    },
    {
      "path": "/auth/inscription/verification-echouee",
      "controller": "AuthController",
      "method": "signupVerifyFail"
    },
    {
      "path": "/auth/reset-mdp",
      "controller": "AuthController",
      "method": "resetPassword"
    },
    {
      "path": "/auth/reset-id",
      "controller": "AuthController",
      "method": "resetId"
    },
    {
      "path": "/contact",
      "controller": "FrontController",
      "method": "contact"
    }
  ]
}